config BACKPORT_MT76x2_COMMON
	tristate
	depends on !MT76x2_COMMON
	select BACKPORT_MT76x02_LIB

config BACKPORT_MT76x2E
	tristate "MediaTek MT76x2E (PCIe) support"
	depends on !MT76x2E
	select BACKPORT_MT76x2_COMMON
	depends on BACKPORT_MAC80211
	depends on PCI
	help
	  This adds support for MT7612/MT7602/MT7662-based wireless PCIe
	  devices, which comply with IEEE 802.11ac standards and support
	  2SS to 866Mbit/s PHY rate.

	  To compile this driver as a module, choose M here.

config BACKPORT_MT76x2U
	tristate "MediaTek MT76x2U (USB) support"
	depends on !MT76x2U
	select BACKPORT_MT76x2_COMMON
	select BACKPORT_MT76x02_USB
	depends on BACKPORT_MAC80211
	depends on USB
	help
	  This adds support for MT7612U-based wireless USB 3.0 dongles,
	  which comply with IEEE 802.11ac standards and support 2SS to
	  866Mbit/s PHY rate.

	  To compile this driver as a module, choose M here.

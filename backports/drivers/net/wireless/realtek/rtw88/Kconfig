menuconfig BACKPORT_RTW88
	tristate "Realtek 802.11ac wireless chips support"
	depends on !RTW88
	depends on BACKPORT_MAC80211
	help
	  This module adds support for mac80211-based wireless drivers that
	  enables Realtek IEEE 802.11ac wireless chipsets.

	  If you choose to build a module, it'll be called rtw88.

if BACKPORT_RTW88

config BACKPORT_RTW88_CORE
	tristate
	depends on !RTW88_CORE

config BACKPORT_RTW88_PCI
	tristate
	depends on !RTW88_PCI

config BACKPORT_RTW88_8822B
	tristate
	depends on !RTW88_8822B

config BACKPORT_RTW88_8822C
	tristate
	depends on !RTW88_8822C

config BACKPORT_RTW88_8723D
	tristate
	depends on !RTW88_8723D

config BACKPORT_RTW88_8821C
	tristate
	depends on !RTW88_8821C

config BACKPORT_RTW88_8822BE
	tristate "Realtek 8822BE PCI wireless network adapter"
	depends on !RTW88_8822BE
	depends on PCI
	select BACKPORT_RTW88_CORE
	select BACKPORT_RTW88_PCI
	select BACKPORT_RTW88_8822B
	help
	  Select this option will enable support for 8822BE chipset

	  802.11ac PCIe wireless network adapter

config BACKPORT_RTW88_8822CE
	tristate "Realtek 8822CE PCI wireless network adapter"
	depends on !RTW88_8822CE
	depends on PCI
	select BACKPORT_RTW88_CORE
	select BACKPORT_RTW88_PCI
	select BACKPORT_RTW88_8822C
	help
	  Select this option will enable support for 8822CE chipset

	  802.11ac PCIe wireless network adapter

config BACKPORT_RTW88_8723DE
	tristate "Realtek 8723DE PCI wireless network adapter"
	depends on !RTW88_8723DE
	depends on PCI
	select BACKPORT_RTW88_CORE
	select BACKPORT_RTW88_PCI
	select BACKPORT_RTW88_8723D
	help
	  Select this option will enable support for 8723DE chipset

	  802.11n PCIe wireless network adapter

config BACKPORT_RTW88_8821CE
	tristate "Realtek 8821CE PCI wireless network adapter"
	depends on !RTW88_8821CE
	depends on PCI
	select BACKPORT_RTW88_CORE
	select BACKPORT_RTW88_PCI
	select BACKPORT_RTW88_8821C
	help
	  Select this option will enable support for 8821CE chipset

	  802.11ac PCIe wireless network adapter

config BACKPORT_RTW88_DEBUG
	bool "Realtek rtw88 debug support"
	depends on !RTW88_DEBUG
	depends on BACKPORT_RTW88_CORE
	help
	  Enable debug support

	  If unsure, say Y to simplify debug problems

config BACKPORT_RTW88_DEBUGFS
	bool "Realtek rtw88 debugfs support"
	depends on !RTW88_DEBUGFS
	depends on BACKPORT_RTW88_CORE
	help
	  Enable debug support

	  If unsure, say Y to simplify debug problems

endif

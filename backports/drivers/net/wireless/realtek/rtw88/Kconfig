menuconfig BACKPORT_RTW88
	tristate "Realtek 802.11ac wireless chips support"
	depends on !RTW88
	depends on BACKPORT_MAC80211
	help
	  This module adds support for mac80211-based wireless drivers that
	  enables Realtek IEEE 802.11ac wireless chipsets.

	  If you choose to build a module, it'll be called rtw88.

if BACKPORT_RTW88

config BACKPORT_RTW88_CORE
	tristate
	depends on !RTW88_CORE
	select WANT_DEV_COREDUMP

config BACKPORT_RTW88_PCI
	tristate
	depends on !RTW88_CORE

config BACKPORT_RTW88_SDIO
	tristate
	depends on !RTW88_SDIO

config BACKPORT_RTW88_USB
	tristate
	depends on !RTW88_USB

config BACKPORT_RTW88_8822B
	tristate
	depends on !RTW88_8822B

config BACKPORT_RTW88_8822C
	tristate
	depends on !RTW88_8822C

config BACKPORT_RTW88_8723X
	tristate
	depends on !RTW88_8723X

config BACKPORT_RTW88_8703B
	tristate
	depends on !RTW88_8703B
	select BACKPORT_RTW88_8723X

config BACKPORT_RTW88_8723D
	tristate
	depends on !RTW88_8723D
	select BACKPORT_RTW88_8723X

config BACKPORT_RTW88_8821C
	tristate
	depends on !RTW88_8821C

config BACKPORT_RTW88_88XXA
	tristate
	depends on !RTW88_88XXA

config BACKPORT_RTW88_8821A
	tristate
	depends on !RTW88_8821A
	select BACKPORT_RTW88_88XXA

config BACKPORT_RTW88_8812A
	tristate
	depends on !RTW88_8812A
	select BACKPORT_RTW88_88XXA

config BACKPORT_RTW88_8822BE
	tristate "Realtek 8822BE PCI wireless network adapter"
	depends on PCI
	depends on !RTW88_8822BE
	select BACKPORT_RTW88_CORE
	select BACKPORT_RTW88_PCI
	select BACKPORT_RTW88_8822B
	help
	  Select this option will enable support for 8822BE chipset

	  802.11ac PCIe wireless network adapter

config BACKPORT_RTW88_8822BS
	tristate "Realtek 8822BS SDIO wireless network adapter"
	depends on !RTW88_8822BS
	depends on MMC
	select BACKPORT_RTW88_CORE
	select BACKPORT_RTW88_SDIO
	select BACKPORT_RTW88_8822B
	help
	  Select this option will enable support for 8822BS chipset

	  802.11ac SDIO wireless network adapter

config BACKPORT_RTW88_8822BU
	tristate "Realtek 8822BU USB wireless network adapter"
	depends on !RTW88_8822BU
	depends on USB
	select BACKPORT_RTW88_CORE
	select BACKPORT_RTW88_USB
	select BACKPORT_RTW88_8822B
	help
	  Select this option will enable support for 8822BU chipset

	  802.11ac USB wireless network adapter

